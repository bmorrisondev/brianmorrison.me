{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby clean&&gatsby build",
    "dev": "gatsby clean&&gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby build&&gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "netlify build && netlify deploy",
    "deploy:prod": "netlify build && netlify deploy --prod"
  },
  "dependencies": {
    "@types/react-helmet": "^6.1.6",
    "dotenv": "^16.0.3",
    "gatsby": "^5.7.0",
    "gatsby-plugin-feed": "^5.7.0",
    "gatsby-plugin-google-adsense": "^1.1.3",
    "gatsby-plugin-image": "^3.7.0",
    "gatsby-plugin-react-helmet": "^6.7.0",
    "gatsby-plugin-sharp": "^5.7.0",
    "gatsby-plugin-styled-components": "^6.7.0",
    "gatsby-source-filesystem": "^5.7.0",
    "gatsby-source-wordpress": "^7.7.0",
    "gatsby-transformer-sharp": "^5.7.0",
    "html-react-parser": "^3.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "gatsby-plugin-postcss": "^6.7.0",
    "joi": "^17.8.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
